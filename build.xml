<?xml version="1.0" ?>
<!-- Configuration of the Ant build system to generate a Jar file --> 
<project name="GUI-lib" default="CreateJar">
  <target name="CreateJar" description="Create Jar file">
        <jar jarfile="jar/GUI-lib.jar" basedir="bin" includes="tech/hackerlife/gui/*.class" excludes="tech/hackerlife/gui/ExampleCode.class" />
  </target>
</project>
